{
  "env": {
    "node": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["standard", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "off",
    "prettier/prettier": "error"
  },
  "ignorePatterns": [".eslintrc.js", "jest.config.js"]
}
